{
  "name": "phaser-npm-webpack-typescript-starter-project",
  "version": "1.1.1",
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "pack:dev": "webpack --config webpack.dev.config.js --progress --colors",
    "pack:dist": "webpack --config webpack.dist.config.js --progress --colors",
    "build:dev": "echo-cli 'Building DEV ...' && npm run lint && npm run pack:dev && echo-cli 'Build Complete ✔'",
    "build:dist": "echo-cli 'Building DIST ...' && npm run lint && npm run pack:dist && echo-cli 'Build Complete ✔'",
    "server:dev": "webpack-dev-server --config webpack.dev.config.js --open",
    "server:dist": "webpack-dev-server --config webpack.dist.config.js --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rroylance/phaser-npm-webpack-typescript-starter-project.git"
  },
  "author": "Richard Roylance <https://github.com/rroylance>",
  "bugs": {
    "url": "https://github.com/rroylance/phaser-npm-webpack-typescript-starter-project/issues"
  },
  "homepage": "https://github.com/rroylance/phaser-npm-webpack-typescript-starter-project#readme",
  "devDependencies": {
    "@types/node": "7.0.4",
    "clean-webpack-plugin": "0.1.15",
    "copy-webpack-plugin": "4.0.1",
    "echo-cli": "1.0.8",
    "git-revision-webpack-plugin": "2.4.1",
    "html-webpack-plugin": "2.28.0",
    "phaser-ce": "2.7.3",
    "ts-loader": "2.0.0",
    "tslint": "4.4.2",
    "typescript": "2.1.5",
    "webfontloader": "1.6.27",
    "webpack": "2.2.1",
    "webpack-dev-server": "1.16.3"
  }
}
